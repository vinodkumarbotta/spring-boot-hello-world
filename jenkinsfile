pipeline {
    agent any
    environment {
       REPO_URL="https://github.com/vinodkumarbotta/jenkinsfile.git"
    }
    stages{
       
        stage("build code"){
            steps{
                script{
                    sh """
                    ls -lrt
                    mvn install
                    """
                }
            }
        }
        stage("Run Unit Test"){
            steps{
                script {
                    sh """
                    mvn test
                    """
                }
            }
        }
    }
}